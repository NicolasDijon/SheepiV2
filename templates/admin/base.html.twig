<!DOCTYPE html>
<html>
    <head>
        <title>{% block title %}SHEEPI - Dashboard{% endblock %}</title>
		<meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link rel="apple-touch-icon" sizes="180x180" href="apple-touch-icon.png">
		<link rel="icon" type="favicon" sizes="32x32" href="favicon-32x32.png">
		<link rel="icon" type="favicon" sizes="16x16" href="favicon-16x16.png">
		<link rel="icon" type="favicon" sizes="192x192" href="android-chrome-192x192.png">
		<link rel="icon" type="favicon" sizes="256x256" href="android-chrome-256x256.png">
        <link rel="icon" type="favicon" sizes="150x150" href="mstile-150x150.png">
        <link rel="icon" type="favicon" sizes="558x558" href="tile.png">
        <link rel="icon" type="favicon" sizes="558x270" href="tile-wide.png">
        <link rel="mask-icon" href="favicon/safari-pinned-tab.svg" color="#a523b8">
        <link href="{{ asset('css/font-face.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/font-awesome-4.7/css/font-awesome.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/font-awesome-5/css/fontawesome-all.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/mdi-font/css/material-design-iconic-font.min.css') }}" rel="stylesheet" media="all">
        <link href="https://fonts.googleapis.com/css?family=Asap&display=swap" rel="stylesheet">
        <link href="{{ asset('vendor/bootstrap-4.1/bootstrap.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/animsition/animsition.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/wow/animate.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/css-hamburgers/hamburgers.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/slick/slick.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/select2/select2.min.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('vendor/perfect-scrollbar/perfect-scrollbar.css') }}" rel="stylesheet" media="all">
        <link href="{{ asset('css/theme.css') }}" rel="stylesheet" media="all">
        {% block stylesheets %}{% endblock %}
    </head>
    <body class="animsition">
        <div class="page-wrapper">
            <!-- HEADER MOBILE-->
            <header class="header-mobile d-block d-lg-none">
                <div class="header-mobile__bar">
                    <div class="container-fluid">
                        <div class="header-mobile-inner">
                            <a class="logo" href="{{ path('admin') }}">
                                <img src="{{ asset('img/icon/logo.png') }}" alt="CoolAdmin" />
                            </a>
                            <button class="hamburger hamburger--slider" type="button">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <nav class="navbar-mobile">
                    <div class="container-fluid">
                        <ul class="navbar-mobile__list list-unstyled">
                            <li class="has-sub">
                                <a class="js-arrow" href="{{ path('admin') }}">
                                    <i class="fas fa-tachometer-alt"></i>Clients
                                </a>
                            </li>
                            <li>
                                <a href="{{ path('admin_caracteristique') }}">
                                    <i class="fas fa-chart-bar"></i>Caractéristiques</a>
                            </li>
                            <li>
                                <a href="{{ path('admin_localisation') }}">
                                    <i class="fas fa-table"></i>Localisation</a>
                            </li>
                            <li>
                                <a href="{{ path('admin_connexion') }}">
                                    <i class="far fa-check-square"></i>Connexions</a>
                            </li>
                            <li>
                                <a href="{{ path('admin_signalement') }}">
                                    <i class="fas fa-calendar-alt"></i>Signalements</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <!-- END HEADER MOBILE-->

            <!-- MENU SIDEBAR-->
            <aside class="menu-sidebar d-none d-lg-block">
                <div class="logo">
                    <a href="#">
                        <img src="{{ asset('img/icon/logo.png') }}" alt="Cool Admin"/>
                    </a>
                </div>
                <div class="menu-sidebar__content js-scrollbar1">
                    <nav class="navbar-sidebar">
                        <ul class="list-unstyled navbar__list">
                            <li>
                                <a class="js-arrow" href="{{ path('admin') }}">
                                    <i class="fas fa-tachometer-alt"></i>Clients
                                </a>
                            </li>
                            <li>
                                <a href="{{ path('admin_caracteristique') }}">
                                    <i class="fas fa-chart-bar"></i>Caractéristiques</a>
                            </li>
                            <li>
                                <a href="{{ path('admin_localisation') }}">
                                    <i class="fas fa-table"></i>Localisation</a>
                            </li>
                            <li>
                                <a href="{{ path('admin_connexion') }}">
                                    <i class="far fa-check-square"></i>Connexions</a>
                            </li>
                            <li>
                                <a href="{{ path('admin_signalement') }}">
                                    <i class="fas fa-calendar-alt"></i>Signalements</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </aside>
            <!-- END MENU SIDEBAR-->

            <!-- PAGE CONTAINER-->
            <div class="page-container">
                <!-- HEADER DESKTOP-->
                <header class="header-desktop">
                    <div class="section__content section__content--p30">
                        <div class="container-fluid">
                            <div class="header-wrap">
                                <form class="form-header" action="" method="POST">
                                    <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                    <button class="au-btn--submit" type="submit">
                                        <i class="zmdi zmdi-search"></i>
                                    </button>
                                </form>
                                <div class="header-button">
                                    <div class="noti-wrap">
                                        <div class="noti__item js-item-menu">
                                            <i class="zmdi zmdi-comment-more"></i>
                                            <span class="quantity">1</span>
                                            <div class="mess-dropdown js-dropdown">
                                                <div class="mess__title">
                                                    <p>Vous avez 2 nouveaux messages</p>
                                                </div>
                                                <div class="mess__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="{{ asset('img/icon/avatar-06.jpg') }}" alt="Michelle Moreno" />
                                                    </div>
                                                    <div class="content">
                                                        <h6>Michelle Moreno</h6>
                                                        <p>Vous à envoyé une photo</p>
                                                        <span class="time">Il y a 3 min</span>
                                                    </div>
                                                </div>
                                                <div class="mess__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="{{ asset('img/icon/avatar-04.jpg') }}" alt="Diane Myers" />
                                                    </div>
                                                    <div class="content">
                                                        <h6>Diane Myers</h6>
                                                        <p>Vous à envoyé un message</p>
                                                        <span class="time">Hier</span>
                                                    </div>
                                                </div>
                                                <div class="mess__footer">
                                                    <a href="#">Voir tout les messages</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="noti__item js-item-menu">
                                            <i class="zmdi zmdi-email"></i>
                                            <span class="quantity">1</span>
                                            <div class="email-dropdown js-dropdown">
                                                <div class="email__title">
                                                    <p>Vous avez 3 nouveaux mails</p>
                                                </div>
                                                <div class="email__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="{{ asset('img/icon/avatar-06.jpg') }}" alt="Cynthia Harvey" />
                                                    </div>
                                                    <div class="content">
                                                        <p>Rencontre pour le dashboard.</p>
                                                        <span>Cynthia Harvey, il y a 3 min</span>
                                                    </div>
                                                </div>
                                                <div class="email__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="{{ asset('img/icon/avatar-05.jpg') }}" alt="Cynthia Harvey" />
                                                    </div>
                                                    <div class="content">
                                                        <p>Rencontre pour le dashboard.</p>
                                                        <span>Cynthia Harvey, Hier</span>
                                                    </div>
                                                </div>
                                                <div class="email__item">
                                                    <div class="image img-cir img-40">
                                                        <img src="{{ asset('img/icon/avatar-04.jpg') }}" alt="Cynthia Harvey" />
                                                    </div>
                                                    <div class="content">
                                                        <p>Rencontre pour le dashboard..</p>
                                                        <span>Cynthia Harvey, 12 avril 2019</span>
                                                    </div>
                                                </div>
                                                <div class="email__footer">
                                                    <a href="#">Voir tout les emails</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="noti__item js-item-menu">
                                            <i class="zmdi zmdi-notifications"></i>
                                            <span class="quantity">3</span>
                                            <div class="notifi-dropdown js-dropdown">
                                                <div class="notifi__title">
                                                    <p>Vous avez 3 notifications</p>
                                                </div>
                                                <div class="notifi__item">
                                                    <div class="bg-c1 img-cir img-40">
                                                        <i class="zmdi zmdi-email-open"></i>
                                                    </div>
                                                    <div class="content">
                                                        <p>Vous avez un email</p>
                                                        <span class="date">12 avril 2019 à 19h00</span>
                                                    </div>
                                                </div>
                                                <div class="notifi__item">
                                                    <div class="bg-c2 img-cir img-40">
                                                        <i class="zmdi zmdi-account-box"></i>
                                                    </div>
                                                    <div class="content">
                                                        <p>Votre compte à été bloqué</p>
                                                        <span class="date">12 avril 2019 à 19h00</span>
                                                    </div>
                                                </div>
                                                <div class="notifi__item">
                                                    <div class="bg-c3 img-cir img-40">
                                                        <i class="zmdi zmdi-file-text"></i>
                                                    </div>
                                                    <div class="content">
                                                        <p>Vous avez un nouveau fichier</p>
                                                        <span class="date">12 avril 2019 à 19h00</span>
                                                    </div>
                                                </div>
                                                <div class="notifi__footer">
                                                    <a href="#">Toute les notifications</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="account-wrap">
                                        <div class="account-item clearfix js-item-menu">
                                            <div class="image">
                                                <img src="{{ asset('img/icon/avatar-01.jpg') }}" alt="John Doe" />
                                            </div>
                                            <div class="content">
                                                <a class="js-acc-btn" href="#">john doe</a>
                                            </div>
                                            <div class="account-dropdown js-dropdown">
                                                <div class="info clearfix">
                                                    <div class="image">
                                                        <a href="#">
                                                            <img src="{{ asset('img/icon/avatar-01.jpg') }}" alt="John Doe" />
                                                        </a>
                                                    </div>
                                                    <div class="content">
                                                        <h5 class="name">
                                                            <a href="#">john doe</a>
                                                        </h5>
                                                        <span class="email">johndoe@example.com</span>
                                                    </div>
                                                </div>
                                                <div class="account-dropdown__body">
                                                    <div class="account-dropdown__item">
                                                        <a href="#">
                                                            <i class="zmdi zmdi-account"></i>Compte</a>
                                                    </div>
                                                    <div class="account-dropdown__item">
                                                        <a href="#">
                                                            <i class="zmdi zmdi-settings"></i>Paramètres</a>
                                                    </div>
                                                    <div class="account-dropdown__item">
                                                        <a href="#">
                                                            <i class="zmdi zmdi-money-box"></i>Factures</a>
                                                    </div>
                                                </div>
                                                <div class="account-dropdown__footer">
                                                    <a href="#">
                                                        <i class="zmdi zmdi-power"></i>Déconnexion</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </header>
                <!-- HEADER DESKTOP-->

        {% block body %}{% endblock %}  

           <!-- Jquery JS-->
            <script src="{{ asset('vendor/jquery-3.2.1.min.js') }}"></script>
            <!-- Bootstrap JS-->
            <script src="{{ asset('vendor/bootstrap-4.1/popper.min.js') }}"></script>
            <script src="{{ asset('vendor/bootstrap-4.1/bootstrap.min.js') }}"></script>
            <!-- Vendor JS       -->
            <script src="{{ asset('vendor/slick/slick.min.js') }}">
            </script>
            <script src="{{ asset('vendor/wow/wow.min.js') }}"></script>
            <script src="{{ asset('vendor/animsition/animsition.min.js') }}"></script>
            <script src="{{ asset('vendor/bootstrap-progressbar/bootstrap-progressbar.min.js') }}">
            </script>
            <script src="{{ asset('vendor/counter-up/jquery.waypoints.min.js') }}"></script>
            <script src="{{ asset('vendor/counter-up/jquery.counterup.min.js') }}">
            </script>
            <script src="{{ asset('vendor/circle-progress/circle-progress.min.js') }}"></script>
            <script src="{{ asset('vendor/perfect-scrollbar/perfect-scrollbar.js') }}"></script>
            <script src="{{ asset('vendor/chartjs/Chart.bundle.min.js') }}"></script>
            <script src="{{ asset('vendor/select2/select2.min.js') }}">
